        android:text="Aplikasi ini adalah aplikasi untuk mencatat berbagai hal"
        android:textAlignment="center"
        android:textSize="24sp"
        tools:textAlignment="center" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Amalia Rizki Febriyani"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Firmaydani Wahyu Alamsyah"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Muhammad Zhorif Fadhlurrohman"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Trischa Nur Laila"
        android:textAlignment="center" />

</LinearLayout>
